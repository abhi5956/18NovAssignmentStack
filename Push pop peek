package com.Stack;
import java.util.Stack;

public class stackProblem {
    public static void main(String[] args) {
        Stack<Integer> stk= new Stack<>();
        stk.push(178);
        stk.push(113);
        stk.push(95);
        stk.push(120);
        System.out.println(stk);
        stk.push( 20);
        System.out.println(stk);
        stk.pop();
        System.out.println(stk);
        System.out.println(stk.peek());
    }
}
